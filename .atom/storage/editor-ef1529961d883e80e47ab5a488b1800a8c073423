p(params, callback) {\n  validateParams(params);\n  log.info('Getting tools group');\n  log.info(params);\n  let query = `\n    MATCH\n    (b:BUNDLE)-[r1]-(c:COUNTRY)-[r2]-(l:LIFECYCLE)-[r3]-(p:PRODUCT_INTENT)-[r4]-(a:ACTIVATED)-[r5]-(s:SEGMENT)-[r6]-(u:USER_ROLE)-[r7]-(t:TOOLS_GROUP)\n\n    WHERE b.name = '${params.bundle}' AND c.name='${params.country}' AND l.name='${params.lifecycle}' AND\n          p.name='${params.intent}' AND a.name='${params.activated}' AND s.name='${params.segment}' AND u.name='${params.role}'\n\n    RETURN t\n  `;\n\n  db.cypherQuery(query, (err, results) => {\n    if (err) {\n      log.error(err);\n      callback(err);\n    }\n\n    // If the path specified isn't in the database, retrieve default tools group\n    if (results.data.length == 0) {\n      getDefaultToolsGroup(callback);\n    } else {\n      log.info('Got tools group');\n      log.info(results.data);\n      callback(null, results);\n    }\n  });\n\n}\n\n\n/*\n* Returns default tools group based on default path, accepts a callback to call on completion\n*/\nfunction getDefaultToolsGroup(callback) {\n  let query = `\n    MATCH\n    (b:BUNDLE)-[r1]-(c:COUNTRY)-[r2]-(l:LIFECYCLE)-[r3]-(p:PRODUCT_INTENT)-[r4]-(a:ACTIVATED)-[r5]-(s:SEGMENT)-[r6]-(u:USER_ROLE)-[r7]-(t:TOOLS_GROUP)\n\n    WHERE r1.default=true AND r2.default=true AND r3.default=true AND\n          r4.default=true AND r5.default=true AND r6.default=true AND r7.default=true\n\n    RETURN t\n  `;\n\n  db.cypherQuery(query, (err, results) => {\n    if (err) {\n      log.error(err);\n      callback(err);\n    }\n\n    // If the default path isn't specified in the database, there is a problem\n    if (results.data.length == 0) {\n      const error = new Error('No default path in database!');\n      log.error(error);\n      callback(error);\n    } else {\n      log.info('Got default tools group');\n      log.info(results.data);\n      callback(null, results);\n    }\n  });\n\n};\n\n/*\n* Sets default tools group based on parameters, accepts a callback to call on completion\n*/\nfunction setDefaultToolsGroup(params, callback) {\n  validateParams(params);\n  log.info('Setting default tools group');\n  log.info(params);\n\n  let query = `\n    MATCH\n    (b:BUNDLE)-[r1]-(c:COUNTRY)-[r2]-(l:LIFECYCLE)-[r3]-(p:PRODUCT_INTENT)-[r4]-(a:ACTIVATED)-[r5]-(s:SEGMENT)-[r6]-(u:USER_ROLE)-[r7]-(t:TOOLS_GROUP)\n\n    WHERE b.name = '${params.bundle}' AND c.name='${params.country}' AND l.name='${params.lifecycle}' AND\n          p.name='${params.intent}' AND a.name='${params.activated}' AND s.name='${params.segment}' AND u.name='${params.role}'\n\n    SET r1.default=true, r2.default=true, r3.default=true,\n          r4.default=true, r5.default=true, r6.default=true, r7.default=true\n\n    RETURN t\n  `;\n\n  db.cypherQuery(query, (err, results) => {\n    if (err) {\n      log.error(err);\n      callback(err);\n    }\n    log.info('Successfully set default tools group');\n    log.info(results.data);\n    callback(null, results);\n  });\n\n};\n\n/*\n* Removes default tools group, accepts a callback to call on completion\n*/\nfunction removeDefaultToolsGroup(callback) {\n  let query = `\n    MATCH\n    (b:BUNDLE)-[r1]-(c:COUNTRY)-[r2]-(l:LIFECYCLE)-[r3]-(p:PRODUCT_INTENT)-[r4]-(a:ACTIVATED)-[r5]-(s:SEGMENT)-[r6]-(u:USER_ROLE)-[r7]-(t:TOOLS_GROUP)\n\n    WHERE r1.default=true AND r2.default=true AND r3.default=true AND\n          r4.default=true AND r5.default=true AND r6.default=true AND r7.default=true\n\n    SET r1.default=NULL, r2.default=NULL, r3.default=NULL, r4.default=NULL, r5.default=NULL, r6.default=NULL, r7.default=NULL\n\n    RETURN t\n  `;\n\n  db.cypherQuery(query, (err, results) => {\n    if (err) {\n      log.error(err);\n      callback(err);\n    }\n    log.info('Removed default tools group');\n    log.info(results.data);\n    callback(null, results);\n  });\n\n}\n\nfunction validateParams(params) {\n  if (!(params.bundle &&\n        params.country &&\n        params.lifecycle &&\n        params.intent &&\n        params.activated &&\n        params.segment &&\n        params.role)) {\n          return false;\n        }\n  return true;\n}\n\nmodule.exports = { getToolsGroup, getDefaultToolsGroup, setDefaultToolsGroup, removeDefaultToolsGroup }","defaultMarkerLayerId":"0","markerLayers":{"0":{"id":"0","maintainHistory":false,"markersById":{},"version":2},"1":{"id":"1","maintainHistory":false,"markersById":{},"version":2},"2":{"id":"2","maintainHistory":true,"markersById":{"1":{"range":{"start":{"row":26,"column":4},"end":{"row":32,"column":12}},"properties":{"type":"selection","goalScreenRange":null,"autoscroll":false},"reversed":false,"tailed":true,"valid":true,"invalidate":"never"}},"version":2}},"nextMarkerLayerId":3,"history":{"version":3,"nextCheckpointId":3,"undoStack":[],"redoStack":[]},"encoding":"utf8","filePath":"/Users/vmaudgalya/projects/merchant-experience-platform/mep-meta/index.js","digestWhenLastPersisted":"da4ce79734ec1ce623f5633c335dd84e03428ff8","preferredLineEnding":null,"nextMarkerId":104,"deserializer":"TextBuffer","version":5},{"id":"051897ec10db8ebdf894db65a87d95e1","text":"/*\n* @author Varun Maudgalya <vmaudgalya@paypalcorp.com>\n* API module test\n*/\n\nconst chai = require('chai');\nconst assert = chai.assert;\nconst expect = chai.expect;\nconst api = require('../index');\nconst badParameters = require('./mocks/mock').badParameters;\nconst goodParameters = require('./mocks/mock').goodParameters;\nconst groupAParameters = require('./mocks/mock').groupAParameters;\n\ndescribe(`mep-meta API`, function() {\n\n  it(`should remove the default tools group`, function(done) {\n\n    api.removeDefaultToolsGroup((err, results) => {\n      if (err) {\n        return done(err);\n      }\n      expect(results.data[0].name).to.equal('C');\n      done();\n    });\n\n  });\n\n  // it(`should set default tools group`, function(done) {\n  //\n  //   api.setDefaultToolsGroup(goodParameters, (err, results) => {\n  //     if (err) {\n  //       